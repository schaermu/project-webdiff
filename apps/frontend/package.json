    "flowbite-vue": "^0.1.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.3",
    "uuid": "^10.0.0",
    "vee-validate": "^4.12.6",
